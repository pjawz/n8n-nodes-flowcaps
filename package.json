{
  "name": "n8n-nodes-flowcaps",
  "version": "0.1.0",
  "description": "Adds visual marker nodes (FlowCaps) to n8n workflows for improved readability.",
  "keywords": [
    "n8n-community-node-package",
    "flowcap",
    "marker",
    "visual",
    "noop"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Peter Joslyn"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pjawz/n8n-nodes-flowcaps"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier --write nodes credentials",
    "lint": "eslint nodes credentials package.json",
    "lint:fix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm run build && pnpm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/FlowCap/FlowCapDone.node.js",
      "dist/nodes/FlowCap/FlowCapEnd.node.js",
      "dist/nodes/FlowCap/FlowCapWarning.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/request-promise-native": "^1.0.17",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.112.0",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}